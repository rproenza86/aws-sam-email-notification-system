{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 5656,
            "cwd": "${workspaceRoot}/src/processNotifications",
            "sourceMaps": true,
            "smartStep": true,
            "localRoot": "${workspaceRoot}/src/processNotifications",
            "remoteRoot": "/var/task",
            "protocol": "inspector",
            "stopOnEntry": false,
            "outFiles": [
                "${workspaceRoot}/src/processNotifications/index.js",
                "${workspaceRoot}/src/processNotifications/saveRecords.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "cwd": "${workspaceRoot}/src/processNotifications",
            "runtimeArgs": ["-r", "ts-node/register"],
            "args": ["${workspaceRoot}/src/processNotifications/index.ts"],
            "sourceMaps": true,
            "smartStep": true
        }
    ]
}
